<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--名称空间-->
<mapper namespace="cn.twopair.mappers.LogMapper">
    <select id="selectAll" resultType="Log">
        SELECT *
        FROM log
        WHERE user = #{user};
    </select>
</mapper>